<div class="container">
    <div class="cata_title">
        <h2>INQUIRY</h2>
    </div>
    <div class="inquire_div">
      <form action= "/home/qna_upload" method= "POST"  enctype="multipart/form-data" accept-charset="UTF-8" style="position:relative;">
        <div class="inquire_name">
            <input type="text" placeholder="请输入名字" style="padding-left:10px;" class="inquire_name" name="inquire_name">
        </div>
        <div class="inquire_name">
            <input type="text" placeholder="请输入您的咨询电话号码"  style="padding-left:10px;" class="inquire_name" name="inquire_phone">
        </div>
         <div class="inquire_name">
            <input type="text" placeholder="请输入电子邮件地址" style="padding-left:10px;" class="inquire_name" name="inquire_email">
        </div>
         <div class="inquire_name">
            <input type="file" placeholder="请上传照片"  style="padding-left:10px;" class="inquire_file" name="inquire_pic">
        </div>
        <div class="inquire_name">
            <input type="text" placeholder="请输入题目" style="padding-left:10px;" class="inquire_name" name="inquire_title">
        </div>
        <div class="inquire_content">
            <input type="textarea" placeholder="请输入内容" style="padding-left:10px;" class="inquire_content" name="inquire_content">
        </div>
                <button type="submit" class="btn btn-success" style="margin-left:auto; margin-right:auto; margin-top:30px; float:right; position:relative; background-color:#c0c0c0; border-color:#c0c0c0;">문의하기</button>
        </form>
    </div>
</div>